{
  "name": "pousada-nossa-praia",
  "version": "1.0.0",
  "description": "development of the static website www.pousadanossapraia.com.br using webpack, handlebars, bootstrap@4",
  "main": "index.html",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "clean": "gulp clean --gulpfile gulpfiles/setup.babel.js",
    "copyAssets": "gulp copyAssets --gulpfile gulpfiles/setup.babel.js",
    "render": "gulp render --gulpfile gulpfiles/render.babel.js",
    "style": "gulp style --gulpfile gulpfiles/bootstrap.babel.js",
    "devBuild": "npm run clean && npm run copyAssets && npm run render && npm run style",
    "devServer": "gulp devServer --gulpfile gulpfiles/devServer.babel.js",
    "devServerSimple": "cd dev && node ../devServer.js",
    "suisse": "cd suisse && node ../devServer.js",
    "quark": "gulp task --gulpfile myfile.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ronalstal/pousada-nossa-praia.git"
  },
  "keywords": [
    "gulp",
    "handlebars",
    "bootstrap"
  ],
  "author": "Ronald Stalder @ronalstal",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ronalstal/pousada-nossa-praia/issues"
  },
  "homepage": "https://github.com/ronalstal/pousada-nossa-praia#readme",
  "dependencies": {
    "jquery": "^3.1.1",
    "lodash": "^4.16.2",
    "moment": "^2.15.1"
  },
  "devDependencies": {
    "babel-core": "^6.14.0",
    "babel-eslint": "^6.1.2",
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-stage-1": "^6.13.0",
    "bootstrap": "^4.0.0-alpha.4",
    "browser-sync": "^2.17.0",
    "del": "^2.2.2",
    "eslint": "^3.5.0",
    "eslint-plugin-babel": "^3.3.0",
    "eslint-plugin-import": "^1.15.0",
    "eslint-plugin-json": "^1.2.0",
    "gulp": "^3.9.1",
    "gulp-compile-handlebars": "^0.6.1",
    "gulp-concat": "^2.6.0",
    "gulp-rename": "^1.2.2",
    "gulp-sass": "^2.3.2",
    "gulp-sourcemaps": "^1.6.0",
    "gulp-util": "^3.0.7",
    "handlebars": "^4.0.5",
    "mime": "^1.3.4",
    "run-sequence": "^1.2.2",
    "through2": "^2.0.1"
  }
}
